@model IEnumerable<SportBook.Models.Team>
@{
    ViewData["Title"] = "MyTeamsWindow";
}

<h1>MyTeamsWindow</h1>

<div class="list-group" id="list-tab" role="tablist">

    @foreach (var item in Model)
    {
        <a class="list-group-item list-group-item-action" asp-action="TeamManagementWindow" asp-route-id="@item.TeamId"  asp-route-name="@item.TeamName"><b>@item.TeamName</b><br />@item.Type</a>
    }
</div>
