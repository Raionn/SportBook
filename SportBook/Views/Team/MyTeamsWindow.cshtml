
@{
    ViewData["Title"] = "MyTeamsWindow";
}

<h1>MyTeamsWindow</h1>

<div class="list-group" id="list-tab" role="tablist">

    @foreach (string name in ViewData["teams"] as IList<string>)
    {
        <a class="list-group-item list-group-item-action" asp-action="TeamManagementWindow" asp-route-id="@name">@name</a>
    }

</div>
