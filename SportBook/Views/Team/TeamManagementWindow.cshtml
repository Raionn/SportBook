
@{
    ViewData["Title"] = "TeamManagementWindow";
}

<h1>TeamManagementWindow</h1>

<li class="list-group-item list-group-item-action">
    @ViewData["team"]
    <a asp-action="TeamManagementWindow" asp-route-id=@ViewData["team"]>Invite</a> |
    <a asp-action="removePlayerFromTeam" asp-route-id=@ViewData["team"]>Leave</a>
</li>